REPO_VERSION=1
# REPO_NAME must be lower case.  Docker requires it for naming.
REPO_NAME="r23"
BASE_PREFIX=${REPO_NAME}-${REPO_VERSION}

BASE_NETWORK_NAME=${BASE_PREFIX}-NETWORK
STORAGE_NETWORK=${BASE_NETWORK_NAME}-storage

BASE_DOCKER_NAME=${BASE_PREFIX}-docker

BASE_STORAGE_DOCKER_NAME=${BASE_DOCKER_NAME}-storage
BASE_STORAGE_CONTAINER_NAME=${BASE_PREFIX}-storage

BASE_SPARK_DOCKER_NAME=${BASE_DOCKER_NAME}-spark
BASE_SPARK_CONTAINER_NAME=${BASE_PREFIX}-spark

WORKER_DOCKER_NAME=${BASE_DOCKER_NAME}
WORKER_CONTAINER_NAME=${BASE_PREFIX}-worker
